@startuml
rectangle "Features" as FEATURES
rectangle "Masked features" as MASKED_FEATURES
rectangle "Selection" as SELECTION
rectangle "New masked features" as NEW_MASKED_FEATURES
rectangle "Prediction" as PREDICTION

' Create a merge point
rectangle " " as MERGE

FEATURES -> MASKED_FEATURES : "Initializer"
MASKED_FEATURES -> SELECTION : "Method"

SELECTION -> MERGE
FEATURES -> MERGE
MERGE -> NEW_MASKED_FEATURES : "Unmasker"

NEW_MASKED_FEATURES -> PREDICTION : "Classifier"
'NEW_MASKED_FEATURES -> MASKED_FEATURES : "time step"
@enduml
